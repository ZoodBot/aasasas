{
  "version": "0.1.0",
  "name": "muzic",
  "electron_version": "1.4.5",
  "description": "A minimal music player",
  "main": "main.js",
  "scripts": {
    "ci:lint": "eslint . --config .eslintrc",
    "ci:transpile": "webpack --config webpack.dev.js --bail",
    "lint": "eslint . --config .eslintrc",
    "lint:fix": "eslint . --config .eslintrc --fix",
    "clean": "find ./dist -maxdepth 1 -type f -delete",
    "serve": "babel-node server.js",
    "start": "ENV=development electron .",
    "dev": "concurrently -k 'webpack-dev-server --config webpack.dev.js' 'npm start'",
    "build:all": "node build.js",
    "build:osx": "webpack --config webpack.prod.js && electron-packager . muZic --platform=darwin --arch=all --overwrite"
  },
  "repository": "https://github.com/nkprince007/muZic-electron",
  "keywords": [
    "Electron",
    "muZic",
    "music player",
    "audio",
    "player"
  ],
  "author": "nkprince007",
  "license": "MIT",
  "devDependencies": {
    "asar": "^0.12.3",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "electron-packager": "^8.2.0",
    "eslint": "^3.9.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.6.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.10.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "classnames": "^2.2.5",
    "electron": "^1.4.5",
    "globby": "^6.1.0",
    "jquery": "^3.2.1",
    "keymirror": "^0.1.1",
    "level-js": "^2.2.4",
    "linvodb3": "^3.25.0",
    "lodash": "^4.16.6",
    "masonry-layout": "^4.1.1",
    "musicmetadata": "^2.0.4",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "teeny-conf": "^1.0.1"
  },
  "optionalDependencies": {
    "fsevents": "^0.1.6"
  }
}
